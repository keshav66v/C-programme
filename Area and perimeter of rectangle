#include<stdio.h>
int main(){
    int l,b;
    float area,perimeter;
    printf("length of rectangle ");
    scanf("%f",&l);
    printf("breadth of rectangle  ");
    scanf("%f",&b);
    area = l*b;
    perimeter = 2*(l+b);
    printf("area = %f\n",area);
    printf("perimeter = %f",perimeter);
    return 0;
}
