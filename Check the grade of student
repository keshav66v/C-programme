#include<stdio.h>
int main(){
    int x;
    printf("marks = ");
    scanf("%d",&x);
    if(x>=90){
        printf("grade is A+");
    }    
    else if (x>=80){
        printf("grade is A");
    } 
    else if (x>=70){
        printf("grade is B+");
    }
    else if (x>=60){
        printf("grade is B");
    }
    else if (x>=50){
        printf("grade is C");
    }
    else if (x<40){
        printf("YOU FAILED");
    }
    return 0;
}   
