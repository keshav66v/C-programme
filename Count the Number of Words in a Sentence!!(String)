#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char str[1000];
    gets(str);
    int words = 0,i;
    for(int i=0;str[i] !='\0'; i++){
        if(isspace(str[i]) && i !=0 && str[i-1] != ' ')
            words++;
    }
    if(str[i-1]==' ')
        printf("%d",words);
    else
        printf("%d",words+1);
    return 0;
}
