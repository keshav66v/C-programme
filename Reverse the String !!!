#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char str[1000],rstr[1000];
    gets(str);
    //reserve string
    int k=0;
    for(int i=strlen(str)-1; i>=0; i--){
        rstr[k++] = str[i];
    }   
    rstr[k] = '\0';
    printf("%s", rstr);
    return 0;
}
