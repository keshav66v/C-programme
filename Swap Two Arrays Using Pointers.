#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n ;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n; i++){
        scanf("%d",a+i);
    }
    int b[n];
    for(int i=0;i<n; i++){
        scanf("%d", b+i);
    }
    for(int i=0;i<n;i++){
        *(a+i) = *(a+i) + *(b+i);
        *(b+i) = *(a+i) - *(b+i);
        *(a+i) = *(a+i) - *(b+i); 
    }
    for(int i=0; i<n;i++){
        printf("%d ",*(a+i));
    }
    printf("\n");
    for(int i=0;i<n; i++){
        printf("%d ", *(b+i));
}
    return 0;
}
