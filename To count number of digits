//WAP to count number of digits in a given number
#include<stdio.h>
int main(){
    int num,count=0;
    printf("Enter the number : ");
    scanf("%d", &num);
    if(num==0){
        count = 1;
    }else{
         while(num !=0){
            num = num/10;
            count++;    
        }     
    } 
    printf("Number of digits are : %d",count);
    return 0;
}      
