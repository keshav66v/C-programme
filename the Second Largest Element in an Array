#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <limits.h>
int main() {

    int i,n;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    int max1 = arr[0];
    int max2 = INT_MIN;
    for(i=1; i<n; i++){
        if(arr[i]>max1){
            max2 = max1;
            max1 = arr[i];    
        }
        else if(arr[i]>max2 && arr [i] !=max1)
            max2 = arr[i];
    }
    if(max2 == INT_MIN)
        printf("No second largest element ");
    else
    printf("%d", max2);
    return 0;
}
